{
    "name": "Save-Restricted-Bot",
    "description": "A Very Simple Telegram Restricted contents saving bot.",
    "keywords": [
        "telegram",
        "files",
        "bot"
    ],
    "repository": "https://github.com/ClubFX/Save-Restricted-Bot",
    "website": "https://github.com/ClubFX/Save-Restricted-Bot",
    "env": {
        "API_ID": {
          "description": "Get this value from my.telegram.org",
          "required": true
        },
        "API_HASH": {
          "description": "Get this value from my.telegram.org",
          "required": true
        },
        "BOT_TOKEN": {
          "description": "Get this from @BotFather",
          "required": true
        },
        "SESSION_STRING": {
          "description": "Your telegram account session. Get it from https://replit.com/@ClubFX/GenerateStringSession?v=1",
          "required": true
        }
    },
    "buildpacks": [
        { 
            "url": "heroku/python"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
}
